<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="root" prefHeight="390.0" prefWidth="287.0" stylesheets="@stylesheets/LeaderCardAction.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane layoutX="114.0" layoutY="272.0">
         <children>
            <Button layoutX="34.0" layoutY="1.0" mnemonicParsing="false" onAction="#nextImage" prefHeight="27.0" prefWidth="19.0" styleClass="arrow-button" />
            <Button layoutX="1.0" layoutY="1.0" mnemonicParsing="false" onAction="#previousImage" prefHeight="27.0" prefWidth="19.0">
               <styleClass>
                  <String fx:value="arrow-button" />
                  <String fx:value="previousButton" />
               </styleClass>
            </Button>
         </children>
      </AnchorPane>
      <ImageView fx:id="leaderImage" fitHeight="219.0" fitWidth="179.0" layoutX="65.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/leaders/LeaderCard2.png" />
         </image>
      </ImageView>
      <Button layoutX="73.0" layoutY="329.0" mnemonicParsing="false" onAction="#activateLeaderCard" text="Activate Leader Power">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="isActive" layoutX="93.0" layoutY="239.0" text="Leader card active">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
